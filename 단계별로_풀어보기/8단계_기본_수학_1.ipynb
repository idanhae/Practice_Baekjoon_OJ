    "#10250 ACM 호텔\n",
    "import sys\n",
    "T = int(input())\n",
    "for i in range(T):\n",
    "    H,W,N = map(int,sys.stdin.readline().split())\n",
    "    if N%H == 0: # 손님 수가 층수의 배수일때 주의\n",
    "        floor = H\n",
    "        room = N // H\n",
    "    else:\n",
    "        floor = N%H\n",
    "        room = N // H+1\n",
