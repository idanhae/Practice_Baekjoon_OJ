    "#1193 분수찾기\n",
    "X = int(input())\n",
    "\n",
    "sum  = 0\n",
    "cnt = 0\n",
    "while sum < X:\n",
    "    cnt+=1\n",
    "    sum+=cnt\n",
    "    \n",
    "diff = sum - X\n",
    "\n",
    "u_list = []\n",
    "d_list = []\n",
    "for i in range(cnt):\n",
    "    u = cnt-i\n",
    "    d = i+1\n",
    "    u_list.append(u)\n",
    "    d_list.append(d)\n",
    "    \n",
    "if cnt % 2 == 0: # even num\n",
    "    print('{}/{}'.format(u_list[diff], d_list[diff]))\n",
    "else: # odd num\n",
    "    print('{}/{}'.format(u_list[-(diff+1)], d_list[-(diff+1)]))"
   ]
